    while (!place_meeting(x+sign(hspd), y, obj_solid)) {
    x+= sign(hspd);
    }
hspd = 0;
}

// Move horizontally
x+= hspd;

// Vertical collisions
if (place_meeting(x, y+vspd, obj_solid)) {
    while (!place_meeting(x, y+sign(vspd), obj_solid)) {
    y+= sign(vspd);
    }
vspd = 0;
}

// Move vertically
y+= vspd;

// Control player's facing direction
if (xprevious &lt; x) {
    image_xscale = 1;
} else if (xprevious &gt; x) {
    image_xscale = -1;
}
    
