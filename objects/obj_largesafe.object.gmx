{
    safestate = 'opened'
    image_index = 2
}
else safestate = 'closed'

//If the safe is opened and has loot, the player is able to steal the loot
if safestate = 'opened'
{
    if place_meeting(x,y,obj_player)
    {
        if loot &gt; 500
        { 
            if keyboard_check_pressed(ord('Q'))
            {
                global.heistloot += 500
                loot -= 500
            }
        }   else if loot &lt; 500
            {
                if keyboard_check_pressed(ord('Q'))
            {
                global.heistloot += loot
                loot = 0  
            }
            }
    }
}

//If all the loot has been stolen, replace safe image with empty safe image
if loot == 0
{
    image_index = 4
}

//Once the safe opens, spawn a random amount of money/other
